{
    "name": "cirlmcesc/laravel-swaggerdoc",
    "description": "Generating OpenAPI 3.0 document based on phpunit test for Laravel.",
    "license": "MIT",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "swagger",
        "phpunit",
        "test"
    ],
    "authors": [
        {
            "name": "Cirlmcesc Ma",
            "email": "cirlmcesc_ma@163.com"
        }
    ],
    "homepage": "https://github.com/cirlmcesc/laravel-swaggerdoc",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^7.0",
        "zircote/swagger-php": "3.*",
        "swagger-api/swagger-ui": "^3.0",
        "symfony/yaml": ">=3.3",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Cirlmcesc\\LaravelSwaggerdoc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cirlmcesc\\LaravelSwaggerdoc\\Providers\\LaravelSwaggerdocServiceProvider"
            ]
        }
    }
}
